{
  "version": "1.0.0",
  "name": "@kentico/gatsby-starter-kontent-lumen",
  "description": "Simple starter blogpost portfolio page for Gatsby and Kontent headless CMS.",
  "repository": "git+https://github.com/kentico/gatsby-starter-kontent-lumen",
  "author": "Martin Makarsky <martinmakarsky@gmail.com> (https://github.com/makma)",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prepare": "node scripts/ncp.js .env.template .env -u",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "build:netlify": "cross-env GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "clean": "gatsby clean",
    "lint": "./node_modules/.bin/eslint --cache --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write './*.js' 'src/**/*.js' 'src/**/*.jsx'",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public"
  },
  "keywords": [
    "gatsby",
    "blog",
    "starter",
    "kontent",
    "headless CMS",
    "CMS"
  ],
  "dependencies": {
    "gatsby-plugin-preact": "^4.0.15",
    "preact": "^10.5.4",
    "preact-render-to-string": "^5.1.10",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@kentico/gatsby-kontent-components": "^6.2.0",
    "@kentico/gatsby-source-kontent": "6.2.0",
    "babel-eslint": "^10.1.0",
    "bluebird": "^3.5.1",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.4",
    "gatsby": "^2.24.76",
    "gatsby-cli": "^2.12.65",
    "gatsby-plugin-catch-links": "^2.3.15",
    "gatsby-plugin-google-analytics": "^2.3.18",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-robots-txt": "^1.5.3",
    "gatsby-plugin-sass": "^2.3.16",
    "gatsby-plugin-sharp": "^2.6.42",
    "gatsby-plugin-sitemap": "^2.4.16",
    "lost": "^8.2.0",
    "ncp": "^2.0.0",
    "node-sass": "^4.14.1",
    "postcss-pxtorem": "^5.1.1",
    "prettier": "^2.1.2",
    "prismjs": "^1.22.0"
  }
}
