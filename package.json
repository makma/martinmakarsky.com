{
  "version": "1.0.0",
  "name": "@kentico/gatsby-starter-kontent-lumen",
  "description": "Simple starter blogpost portfolio page for Gatsby and Kontent headless CMS.",
  "repository": "git+https://github.com/kentico/gatsby-starter-kontent-lumen",
  "author": "Martin Makarsky <martinmakarsky@gmail.com> (https://github.com/makma)",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prepare": "node scripts/ncp.js .env.template .env -u",
    "develop": "gatsby clean && gatsby develop",
    "build": "gatsby clean && gatsby build",
    "build:netlify": "cross-env GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "clean": "gatsby clean",
    "lint": "./node_modules/.bin/eslint --cache --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write './*.js' 'src/**/*.js' 'src/**/*.jsx'",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "serve": "gatsby serve"
  },
  "keywords": [
    "gatsby",
    "blog",
    "starter",
    "kontent",
    "headless CMS",
    "CMS"
  ],
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^3.3.1",
    "@fingerprintjs/fingerprintjs-pro": "^3.5.4",
    "@fpjs-incubator/botd-agent": "^0.1.22",
    "@reach/router": "^1.3.4",
    "gatsby-plugin-image": "^1.13.0",
    "gatsby-plugin-preact": "^5.13.0",
    "preact": "^10.5.14",
    "preact-render-to-string": "^5.1.19",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@kentico/gatsby-kontent-components": "8.0.0-beta.1",
    "@kentico/gatsby-source-kontent": "8.0.0-beta.1",
    "babel-eslint": "^10.1.0",
    "bluebird": "^3.7.2",
    "cross-env": "^7.0.3",
    "dotenv": "^8.6.0",
    "eslint": "^8.1.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.25.1",
    "gatsby": "^4.0.2",
    "gatsby-cli": "^4.0.0",
    "gatsby-plugin-catch-links": "^4.0.0",
    "gatsby-plugin-google-analytics": "^4.0.0",
    "gatsby-plugin-react-helmet": "^5.0.0",
    "gatsby-plugin-robots-txt": "^1.6.14",
    "gatsby-plugin-sass": "^5.0.0",
    "gatsby-plugin-sharp": "^4.0.1",
    "gatsby-plugin-sitemap": "^5.0.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.25",
    "lodash": "^4.17.21",
    "lost": "9.0.0-0",
    "ncp": "^2.0.0",
    "node-sass": "^6.0.1",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^2.4.1",
    "prismjs": "^1.25.0"
  }
}
